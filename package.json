{
  "name": "chat",
  "version": "0.1.0",
  "hommepage": "https://zhoubichuan.github.io/Project-Vue-weChat/",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "dev": "cross-env NODE-ENV=development node build/webpack.deb.conf.js",
    "build": "vue-cli-service build",
    "build1": "cross-env NODE_ENV=production webpack --config build/webpack.prod.config.js",
    "build --qa": "cross-env NODE_ENV=testing node build/build.js",
    "build --prod": "cross-env NODE_ENV=production node build/build.js",
    "lint": "vue-cli-service lint",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "generateAnalyzFile": "webpack --profile --json > stats.json",
    "analyz": "cross-env NODE_ENV=production npm_config_report=true npm run build"
  },
  "dependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "axios": "^0.18.0",
    "babel": "^6.23.0",
    "babel-loader": "^8.0.5",
    "cache-loader": "^2.0.1",
    "core-js": "^2.6.5",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "vue": "^2.6.10",
    "vue-router": "^3.0.2",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "babel-eslint": "^10.0.1",
    "cross-env": "^6.0.3",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "gh-pages": "^2.1.1",
    "html-withimg-loader": "^0.1.16",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
